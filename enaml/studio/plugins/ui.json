{
	"id": "enaml.studio.ui",
	"name": "Enaml Studio UI",
	"description": "The Enaml Studio ui plugin.",
	"class": "enaml.studio.ui.ui_plugin.UIPlugin",
	"extensionPoints": [
		{
			"id": "application",
			"name": "Studio Application",
			"description": "The Enaml Application factory.",
			"interface": "enaml.studio.ui.application_factory.ApplicationFactory"
		},
		{
			"id": "window",
			"name": "Studio Window",
			"description": "The primary StudioWindow factory.",
			"interface": "enaml.studio.ui.window_factory.WindowFactory"
		},
		{
			"id": "icon",
			"name": "Window Icon",
			"description": "The primary application window icon.",
			"interface": "enaml.studio.ui.icon_provider.IconProvider",
			"schema": "file://schemas/icon.json"
		},
		{
			"id": "title",
			"name": "Window Title",
			"description": "The primary application window title.",
			"interface": "enaml.studio.ui.title_provider.TitleProvider",
			"schema": "file://schemas/title.json"
		},
		{
			"id": "menus",
			"name": "Window Menus",
			"description": "The primary application window menus.",
			"interface": "enaml.studio.ui.menu_provider.MenuProvider"
		},
		{
			"id": "content",
			"name": "Window Content",
			"description": "The primary application window content.",
			"interface": "enaml.studio.ui.content_provider.ContentProvider"
		}
	],
	"extensions": [
		{
			"id": "defaultApplication",
			"point": "enaml.studio.ui.application",
			"name": "Default Application Factory",
			"description": "The default studio application factory.",
			"class": "enaml.studio.ui.application_factory.ApplicationFactory",
			"rank": -1000
		},
		{
			"id": "defaultWindow",
			"point": "enaml.studio.ui.window",
			"name": "Default Window Factory",
			"description": "The default studio window factory.",
			"class": "enaml.studio.ui.window_factory.WindowFactory",
			"rank": -1000
		},
		{
			"id": "defaultTitle",
			"point": "enaml.studio.ui.title",
			"name": "Default Window Title",
			"description": "The default studio window title.",
			"rank": -1000,
			"config": {
				"title": "Enaml Studio"
			}
		}
	]
}
