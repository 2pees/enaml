{
	"id": "enaml.workbench.core",
	"name": "Enaml Workbench Core",
	"description": "The Enaml workbench core plugin.",
	"class": "enaml.workbench.core.core_plugin.CorePlugin",
	"extensionPoints": [
		{
			"id": "loaders",
			"name": "URL Loaders",
			"description": "URL loaders are used to load data from a url.",
			"interface": "enaml.workbench.core.url_loader.URLLoader",
			"schema": "file://schemas/loader.json"
		}
	],
	"extensions": [
		{
			"id": "defaultFileLoader",
			"point": "enaml.workbench.core.loaders",
			"name": "Default File Loader",
			"description": "The default url loader for the local filesystem.",
			"class": "enaml.workbench.core.file_loader.FileLoader",
			"schemeName": "file",
			"rank": -1000,
		}
	]
}
